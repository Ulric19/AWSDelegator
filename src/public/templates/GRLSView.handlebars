<table id="EC2InstanceTable" class="display">
	<thead>
		<tr class="dark-row">
			<th>User Name</th>
			<th>Group Name</th>
			<th>Instance ID</th>
			<th>Type</th>
			<th>State</th>
		</tr>
	</thead>
	<tbody id="instanceData">
	{{#each ec2instances}}
		<tr>
			<td>{{this.userName}}</td>
			<td>{{this.groupName}}</td>
			<td >{{this.instance}}</td>
			<td>{{this.instanceType}}</td>
			<td>{{this.state}}</td>
		</tr>
	{{/each}}
	</tbody>	
</table>