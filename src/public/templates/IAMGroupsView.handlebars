<table id="GroupsTable" class="hover">
	<thead>
		<tr class="dark-row">
			<th>GroupName</th>
			<th>GroupId</th>
			<th>Arn</th>
			<th>CreateDate</th>
			<th>Amount</th>
		</tr>
	</thead>
	<tbody id="instanceData">
	{{#each instances}}
		<tr>
			<td >{{this.groupName}}</td>
			<td>{{this.groupId}}</td>
			<td>{{this.arn}}</td>
			<td>{{this.createDate}}</td>
			<td>{{this.amount}}</td> 
		</tr>
	{{/each}}
	</tbody>	
</table>
