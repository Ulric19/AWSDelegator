
<div class="CPU">
	<table id="CPUTable" class="tablesorter-metro" cellspacing="1">
		<thead>
			<tr class="dark-row" role="row">
				<th class="tablesorter-header" data-column="0">
					<div class="tablesorter-header-inner">Instance ID</div>
				</th>
				<th class="tablesorter-header" data-column="1">
					<div class="tablesorter-header-inner">CPU Usage</div>
				</th>

			</tr>
		</thead>
		<tbody id="cpuData">
		{{#each cpuMetrics}}
			<tr class="tablesorter-body">
				<td>{{this.instance}}</td>
				<td>{{this.cpu}}</td>

			</tr>
		{{/each}}
		</tbody>	
	</table>
</div>